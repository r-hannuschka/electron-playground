{
  "name": "electron-playground",
  "version": "0.0.0",
  "description": "electron sandbox for testing",
  "main": "./dist/extension.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "vscode:prepublish": "rimraf ./dist && webpack --config ./bin/webpack.conf.js --mode production",
    "prebuild": "rimraf ./dist",
    "prestart": "rimraf ./dist",
    "build": "webpack --config ./bin/webpack.conf.js",
    "start": "webpack --config ./bin/webpack.conf.js && electron ./dist/electron-app.js"
  },
  "publisher": "qmasters",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscodeElectron.helloWorld"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscodeElectron.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "author": "Ralf Hannuschka",
  "license": "ISC",
  "devDependencies": {
    "@types/cli-progress": "3.8.0",
    "@types/node-fetch": "2.5.7",
    "@types/unzipper": "^0.10.3",
    "@types/vscode": "^1.52.0",
    "@vue/compiler-sfc": "^3.0.4",
    "cli-progress": "3.8.2",
    "core-js": "^3.8.1",
    "electron": "^11.1.1",
    "eslint": "7.13.0",
    "extract-zip": "^2.0.1",
    "html-webpack-plugin": "5.0.0-alpha.14",
    "node-fetch": "2.6.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "3.0.2",
    "ts-loader": "8.0.11",
    "tsconfig-paths-webpack-plugin": "3.3.0",
    "tsyringe": "4.4.0",
    "typescript": "4.0.5",
    "webpack": "5.4.0",
    "webpack-cli": "4.2.0"
  },
  "dependencies": {}
}
